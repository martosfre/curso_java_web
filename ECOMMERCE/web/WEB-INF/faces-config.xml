<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	<navigation-rule>
		<display-name>pages/menu.xhtml</display-name>
		<from-view-id>/pages/menu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cliente</from-outcome>
			<to-view-id>/pages/admin/cliente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/menu.xhtml</display-name>
		<from-view-id>/pages/menu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>producto</from-outcome>
			<to-view-id>/pages/inventario/producto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
