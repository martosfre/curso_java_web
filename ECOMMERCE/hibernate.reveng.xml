<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	</type-mapping>
	<table-filter match-schema="ecommerce" match-name=".*" />
	<table catalog="ecommerce" name="cliente">
		<column name="id_cli"></column>
		<column name="correo_per"></column>
		<column name="nombre_per"></column>
		<column name="telefono_per"></column>
		<column name="identificacion_per"></column>
		<column name="apellido_per"></column>
		<column name="direccion_per"></column>
		<column name="cuota_ven_cli"></column>
		<column name="fecha_nac_per"></column>
	</table>
	<table catalog="ecommerce" name="detalle_factura">
		<column name="id_detfac"></column>
		<column name="subtotal_detfac"></column>
		<column name="total_detfac"></column>
		<column name="id_prov"></column>
		<column name="iva_detfac"></column>
		<column name="can_detfac"></column>
		<column name="id_pro"></column>
		<column name="obs_detfac"></column>
		<column name="id_fac"></column>
	</table>
	<table catalog="ecommerce" name="factura">
		<column name="id_fac"></column>
		<column name="subtotal_fac"></column>
		<column name="fecha_fac"></column>
		<column name="iva_fac"></column>
		<column name="fac_numero"></column>
		<column name="total_fac"></column>
		<column name="id_cli"></column>
	</table>
	<table catalog="ecommerce" name="producto">
		<column name="id_pro"></column>
		<column name="descripcion_pro"></column>
		<column name="precio_pro"></column>
		<column name="fecha_cad_pro"></column>
		<column name="nombre_pro"></column>
		<column name="id_tip_pro"></column>
	</table>
	<table catalog="ecommerce" name="producto_has_proveedor">
		<column name="id_pro"></column>
		<column name="id_prov"></column>
	</table>
	<table catalog="ecommerce" name="proveedor">
		<column name="id_prov"></column>
		<column name="nombre_per"></column>
	</table>
	<table catalog="ecommerce" name="rol">
		<column name="id_rol"></column>
		<column name="descripcion_rol"></column>
		<column name="nombre_rol"></column>
	</table>
	<table catalog="ecommerce" name="tipo_producto">
		<column name="id_tip_pro"></column>
		<column name="nombre_tip_pro"></column>
		<column name="descripcion_tip_pro"></column>
	</table>
	<table catalog="ecommerce" name="usuario">
		<column name="id_usu"></column>
		<column name="clave_usu"></column>
		<column name="id_rol"></column>
		<column name="nombre_usu"></column>
	</table>
</hibernate-reverse-engineering>